{
  "pname": "dbeaver-community",
  "version": "24.2.1",
  "artifacts": [
    {
      "type": "uninstall",
      "value": [
        {
          "type": "signal",
          "value": [{ "signal": "TERM", "pid": "org.jkiss.dbeaver.core.product" }]
        }
      ]
    },
    { "type": "app", "value": { "name": "DBeaver.app" } },
    { "type": "binary", "value": { "name": "$APPDIR/DBeaver.app/Contents/MacOS/dbeaver" } },
    {
      "type": "zap",
      "value": [
        {
          "type": "trash",
          "value": [
            "~/Library/Caches/org.jkiss.dbeaver.core.product",
            "~/Library/DBeaverData",
            "~/Library/HTTPStorages/org.jkiss.dbeaver.core.product",
            "~/Library/Preferences/org.jkiss.dbeaver.core.product.plist",
            "~/Library/Saved Application State/org.jkiss.dbeaver.core.product.savedState"
          ]
        }
      ]
    }
  ],
  "src": {
    "url": "https://dbeaver.io/files/24.2.1/dbeaver-ce-24.2.1-macos-aarch64.dmg",
    "sha256": "8341ba7ea47be40a0e133958afa31b4c12fc690fe1590b85cb0d46db0f7f2655"
  },
  "installPhase": [
    "mkdir -p \"$out/Applications\" && cp -r \"DBeaver.app\" \"$out/Applications/DBeaver.app\"",
    "mkdir -p \"$out/bin\" && ln -s \"$out/Applications/DBeaver.app/Contents/MacOS/dbeaver\" \"$out/bin/dbeaver\""
  ],
  "meta": {
    "description": "Universal database tool and SQL client",
    "homepage": "https://dbeaver.io/"
  }
}
